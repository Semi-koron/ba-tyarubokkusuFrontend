{
  "name": "ba-tyarubox-frontend",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"start": "vite preview",
		"lint:stylelint": "stylelint 'src/**/*.css'",
		"format:stylelint": "stylelint 'src/**/*.css' --fix",
		"lint:markuplint": "markuplint 'src/**/*.{tsx,ts,jsx,js,html}'",
		"format:markuplint": "markuplint 'src/**/*.{tsx,ts,jsx,js,html}' --fix",
		"ci:lint": "npm-run-all lint:*",
		"ci:test": "npm-run-all test:*",
		"lint:biome": "biome lint --apply --no-errors-on-unmatched .",
		"format:biome": "biome format --write --no-errors-on-unmatched .",
		"lint": "run-p lint:*",
		"format": "run-p format:*",
		"lint:eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"prepare": "husky"
	},
  "dependencies": {
    "@mediapipe/tasks-vision": "^0.10.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
}
